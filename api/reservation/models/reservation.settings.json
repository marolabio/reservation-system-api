{
  "kind": "collectionType",
  "collectionName": "reservations",
  "info": {
    "name": "reservation"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "customer": {
      "model": "customer"
    },
    "reserved_room": {
      "type": "component",
      "repeatable": true,
      "component": "reservation.reserved-room"
    },
    "checkin": {
      "type": "date"
    },
    "checkout": {
      "type": "date"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "reserved",
        "checkedin",
        "checkedout"
      ],
      "default": "reserved"
    }
  }
}
